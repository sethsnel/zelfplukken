---
import { storyblokEditable } from "@storyblok/astro";
import MenuItem from "../components/MenuItem.astro";

const { blok: { titel, link, kleur, afbeelding} } = Astro.props;

const item = { title: titel, href: '/' + link.cached_url, bgColor: kleur, image: afbeelding.filename }
const props = { item }
---

<MenuItem {...storyblokEditable(Astro.props.blok)} { ...props } />

